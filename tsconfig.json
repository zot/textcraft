{
    "compilerOptions": {
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "esnext",
        "module": "esnext",
        "baseUrl": "html/",
        "outDir": "html/js",
        "rootDir": "html/ts",
        "paths": {
            "/js/*.js": ["js/*"],
        },
    },
    "include": [
        "html/ts/**/*"
    ]
}
